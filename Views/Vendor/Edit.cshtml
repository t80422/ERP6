            $('.data-item').each(function (i, v) {
                $(this).removeClass('active');
            });

            let vendorNo = $("#VendorNo").val();

            if (vendorNo) {
                //找出此筆資料
                $('.data-item').each(function (i, v) {
                    if ($(v).children('.f-1').text() == vendorNo) {
                        $(this).addClass('active');

                        return;
                    }
                });

                let el = document.querySelector('.data-item.active');

                if (el) {

                    el.scrollIntoView(true);
                }
            }
        });

        //清除編輯資料
        $('.btn-clean.edit').on('click', function () {
            //先清除所有資料的綁定狀態
            $('.data-item').each(function (i, v) {
                $(this).removeClass('active');
            });

            //清除資料
            $("#VendorNo").val('');
            $("#Uniform").val('');
            $("#Vename").val('');
            $("#Company").val('');
            $("#Dlien").val('');
            $("#Invocomp").val('');
            $("#Payment").val('');

            $("#Tel1").val('');
            $("#Boss").val('');
            $("#Fax").val('');
            $("#ChehkDay").val('');
            $("#Tel2").val('');
            $("#Sales").val('');
            $("#Mobile").val('');

            $("#TaxType").val('');
            $("#Email").val('');
            $("#Taxrate").val('');
            $("#Invoaddr").val('');
            $("#Ntus").val('');
            $("#Factaddr").val('');
            $("#Discount").val('');

            $("#Product").val('');
            $("#PriceType").val('');
            $("#Payaccount").val('');
            $("#Paybank").val('');
            $("#Www").val('');
            $("#Memo").val('');
        });

        //編輯資料
        $(function () {
            $('#btnSubmit').on('click', function () {
                //開啟確認視窗
                $(".popup-wrap.check").addClass("is-active");
                $(".popup-overlay").addClass("is-active");

                //按下取消按鈕
                $('.btn-cancel.small.check').on('click', function () {

                    $(".popup-wrap.check").removeClass("is-active");
                    $(".popup-overlay").removeClass("is-active");

                    window.location.href = "@Url.Action("Index", "Vendor")";
                });

                //按下確認按鈕
                $('#btnCheck').on('click', function () {

                    var strEmail = $("#Email").val()

                    let emailRule = /^\w+((-\w+)|(\.\w+))*\@@[A-Za-z0-9]+((\.|-)[A-Za-z0-9]+)*\.[A-Za-z]+$/;

                    if (strEmail.search(emailRule) == -1) {
                        alert("Email格式錯誤");

                        return;
                    }

                    let fromData = $("#form").serialize();

                    $.ajax({
                        url: "@Url.Action("Edit", "Vendor")",
                        data: fromData,
                        type: 'POST',
                        success: function (data) {
                            if (data.isTrue) {
                                alert("編輯成功");

                                let editVendorNo = data.vendorNo;

                                window.location.href = "@Url.Action("Index", "Vendor")/?VendorNo=" + editVendorNo;

                            }
                            else {
                                alert(data.errorMessage);
                            }
                        }
                    });

                    $(".popup-wrap.check").removeClass("is-active");
                    $(".popup-overlay").removeClass("is-active")

                });
            });
        });

        //離開
        $(function () {
            $(".tool-btn-leave").on('click', function () {
                window.location.href = "@Url.Action("Index","Vendor")";
            });
        });

        // tabs
        $(function () {
            $(".tabs-item:first").addClass("is-active");
            $(".tabs-content:first").addClass("is-active");
            $(".tabs-item").click(function () {
                $(this).addClass("is-active");
                $(this).siblings().removeClass("is-active");
                $(".tabs-content").siblings().removeClass("is-active");
                $(".tabs-content").eq($(this).index()).addClass("is-active");
            });
        });

        // 會計科目-進項科目-視窗畫面 觸發
        $(function () {
            $(".btn-opt").on("click", function (e) {
                $("#demo1").addClass("is-active");
                $(".popup-overlay").addClass("is-active");
            });
        });
        // 會計科目-進項科目-視窗畫面 觸發
        $(function () {
            $(".tool-btn-change").on("click", function (e) {
                $("#demo2").addClass("is-active");
                $(".popup-overlay").addClass("is-active");
            });
        });
    </script>
</body>
