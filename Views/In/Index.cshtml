                                                    </div>
                                                    <div class="form-flex f-s">
                                                        <div class="form-input">
                                                            <label for="">進貨單號</label>
                                                            @Html.TextBoxFor(x => x.InNo, new { @readonly = "readonly" })
                                                        </div>
                                                    </div>
                                                </div>
                                                <div class="form-row">
                                                    <div class="form-flex f-s">
                                                        <div class="form-input">
                                                            <label for="">進貨類別</label>
                                                            <div class="option">
                                                                @Html.DropDownListFor(x => x.InType, ViewBag.InTypeList as IEnumerable<SelectListItem>, new { @class = "form-control" })
                                                            </div>
                                                        </div>
                                                    </div>
                                                    <div class="form-flex f-s">
                                                        <div class="form-input">
                                                            <label for="">發票號碼</label>
                                                            @Html.TextBoxFor(x => x.InvoiceNo, new { @readonly = "readonly" })
                                                        </div>
                                                    </div>
                                                    <div class="form-flex f-s">
                                                        <div class="form-input">
                                                            <label for="">幣別</label>
                                                            <select name="Ntus" id="Ntus">
                                                                <option value="">NTD</option>
                                                            </select>
                                                        </div>
                                                    </div>
                                                </div>
                                                <div class="form-row">
                                                    <div class="form-flex f-s">
                                                        <div class="form-input">
                                                            <label for="">帳款月份</label>
                                                            <div class="option">
                                                                @Html.TextBoxFor(x => x.Paymonth, new { @readonly = "readonly", type = "date" })
                                                            </div>
                                                        </div>
                                                    </div>
                                                </div>
                                                <div class="form-row">
                                                    <div class="form-flex f-s">
                                                        <div class="form-input">
                                                            <label for="">備註</label>
                                                            <div class="option">
                                                                @Html.DropDownListFor(x => x.Memo, ViewBag.MemoList as SelectList, "請選擇", htmlAttributes: new { @class = "form-control" })
                                                            </div>
                                                        </div>
                                                    </div>
                                                </div>
                                                <div class="form-link">
                                                    <button type="button" id="AddDetail" class="link-add">
                                                        <i></i><span>清單資料</span>
                                                    </button>
                                                </div>
                                                <div class="table2-wrap pad-t20 pad-b20 pad-r30 pad-l30">
                                                    <div class="table2-title">
                                                        <div class="tabs-item">明細資料</div>
                                                    </div>
                                                    <div class="table2-overflow">
                                                        <div class="table2-main">
                                                            <div class="tabs-content">
                                                                <div class="table-width w-1300">
                                                                    <table>
                                                                        <tr class="sticky">
                                                                            <th class="p-5 ta-center">
                                                                                <span>序號</span>
                                                                            </th>
                                                                            <th class="p-9 ta-left">
                                                                                <span>採購單號</span>
                                                                            </th>
                                                                            <th class="p-9 ta-left">
                                                                                <span>產品編號</span>
                                                                            </th>
                                                                            <th class="p-18 ta-left">
                                                                                <span>品名規格</span>
                                                                            </th>
                                                                            <th class="p-5 ta-center">
                                                                                <span>數量</span>
                                                                            </th>
                                                                            <th class="p-5 ta-center">
                                                                                <span>單位</span>
                                                                            </th>
                                                                            <th class="p-5 ta-center">
                                                                                <span>稅別</span>
                                                                            </th>
                                                                            <th class="p-5 ta-right">
                                                                                <span>單價</span>
                                                                            </th>
                                                                            <th class="p-6 ta-right">
                                                                                <span>折扣率</span>
                                                                            </th>
                                                                            <th class="p-6 ta-right">
                                                                                <span>金額</span>
                                                                            </th>
                                                                            <th class="p-17 ta-left">
                                                                                <span>備註</span>
                                                                            </th>
                                                                        </tr>
                                                                        @if (Model.in20List != null && Model.in20List.Count > 0)
                                                                        {
                                                                            foreach (var in20Item in Model.in20List)
                                                                            {
                                                                                <tr>
                                                                                    <td class="ta-center">
                                                                                        <span>@in20Item.Serno</span>
                                                                                    </td>
                                                                                    <td class="ta-center">
                                                                                        <span>@in20Item.InNo</span>
                                                                                    </td>
                                                                                    <td class="ta-left">
                                                                                        <span>@in20Item.PartNo</span>
                                                                                    </td>
                                                                                    <td class="ta-left">
                                                                                        <span>@in20Item.Spec</span>
                                                                                    </td>
                                                                                    <td class="ta-center">
                                                                                        <span>@in20Item.Qty</span>
                                                                                    </td>
                                                                                    <td class="ta-center">
                                                                                        <span>@in20Item.Unit</span>
                                                                                    </td>
                                                                                    <td class="ta-center">
                                                                                        <span>@in20Item.TaxType</span>
                                                                                    </td>
                                                                                    <td class="ta-right">
                                                                                        <span>@in20Item.Price</span>
                                                                                    </td>
                                                                                    <td class="ta-right">
                                                                                        <span>@in20Item.Discount</span>
                                                                                    </td>
                                                                                    <td class="ta-right">
                                                                                        <span>@in20Item.Amount</span>
                                                                                    </td>
                                                                                    <td class="ta-right">
                                                                                        <span>@in20Item.Memo</span>
                                                                                    </td>
                                                                                </tr>
                                                                            }
                                                                        }
                                                                    </table>
                                                                </div>
                                                            </div>
                                                        </div>
                                                    </div>
                                                </div>
                                                <div class="form-row">
                                                    <div class="form-flex f-1">
                                                        <div class="form-input">
                                                            <label for="">應稅合計</label>
                                                            @Html.TextBoxFor(x => x.Total1, new { @readonly = "readonly" })
                                                        </div>
                                                    </div>
                                                    <div class="form-flex f-1">
                                                        <div class="form-input">
                                                            <label for="">免稅合計</label>
                                                            @Html.TextBoxFor(x => x.Total0, new { @readonly = "readonly" })
                                                        </div>
                                                    </div>
                                                    <div class="form-flex f-s">
                                                        <div class="form-input">
                                                            <label for="">合計金額</label>
                                                            @Html.TextBoxFor(x => x.Total, new { @readonly = "readonly" })
                                                        </div>
                                                    </div>
                                                </div>
                                                <div class="form-row">
                                                    <div class="form-flex f-1">
                                                        <div class="form-input">
                                                            <label for="">已付金額</label>
                                                            @Html.TextBoxFor(x => x.YesGet, new { @readonly = "readonly" })
                                                        </div>
                                                    </div>
                                                    <div class="form-flex f-s">
                                                        <div class="form-input col-2">
                                                            <label for="">現金扣%</label>
                                                            @Html.TextBoxFor(x => x.Discount, new { @readonly = "readonly" })
                                                        </div>
                                                        <label for=""> </label>
                                                        <div class="form-input col-2">
                                                            @Html.TextBoxFor(x => x.CashDis, new { @readonly = "readonly" })
                                                        </div>
                                                    </div>
                                                    <div class="form-flex f-s">
                                                        <div class="form-input">
                                                            <label for="">折讓金額</label>
                                                            @Html.TextBoxFor(x => x.SubTot, new { @readonly = "readonly" })
                                                        </div>
                                                    </div>
                                                    <div class="form-flex f-s">
                                                        <div class="form-input">
                                                            <label for="">未付金額</label>
                                                            @Html.TextBoxFor(x => x.NotGet, new { @readonly = "readonly" })
                                                        </div>
                                                    </div>
                                                </div>
                                                <div class="form-row">
                                                    <div class="form-flex f-1">
                                                        <div class="form-input">
                                                            <label for="">輸入人員</label>
                                                            @Html.TextBoxFor(x => x.Userid, new { @readonly = "readonly" })
                                                        </div>
                                                    </div>
                                                    <div class="form-flex f-s">
                                                        <div class="form-input">
                                                            <label for="">結帳</label>
                                                            <div class="option">
                                                                @Html.TextBoxFor(x => x.YnClose, new { @readonly = "readonly" })
                                                            </div>
                                                        </div>
                                                    </div>
                                                    <div class="form-flex f-s">
                                                        <div class="form-input">
                                                            <label for="">過庫存</label>
                                                            <div class="option">
                                                                @Html.TextBoxFor(x => x.StockPass, new { @readonly = "readonly" })
                                                            </div>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="btn-bottom">
                                            <button type="button" class="btn-clean search">清除條件</button>
                                            <button type="submit" class="btn-submit">確定</button>
                                        </div>
                                    </div>
                                </div>
                            </form>
                        </div>
                    </div>
                </div>
            </div>
        </main>
        <!-- popup -->
        <!-- 確認視窗 -->
        <div class="popup-wrap check">
            <div class="p-form">
                <p>是否確定執行?</p>
                <div class="p-btn pad-0">
                    <button type="button" class="btn-cancel small check">取消</button>
                    <button type="button" class="btn-submit small check">確定</button>
                </div>
            </div>
        </div>
        <!-- overlay -->
        <div class="popup-overlay"></div>
    </article>
    <!-- JS -->
    <script src="~/js/script.js"></script>
    <script>

        //廠商資料
        $('#VendorNo').combobox({
            url: "@Url.Action("AjaxVendorForEasyUI", "Ajaxs")",
            required: false,
            valueField: 'id',
            textField: 'text'
        });

        //初始資料
        $(function () {
            //先清除所有資料的綁定狀態
            $('.data-item').each(function (i, v) {
                $(this).removeClass('active');
            });

            let inNo = $("#InNo").val();

            if (inNo) {
                //找出此筆資料
                $('.data-item').each(function (i, v) {
                    if ($(v).children('.f-1.in10').text() == inNo) {
                        $(this).addClass('active');

                        return;
                    }
                });

                let el = document.querySelector('.data-item.active');

                if (el) {

                    el.scrollIntoView(true);
                }
            }

            // 列印
             $('.tool-btn-print').on('click', function () {
                var InNo = $('#InNo').val()
                if (!InNo) {
                    alert("找不到編輯資料!");
                } else {
                    window.location.href = '@Url.Action("Print","In")' + "?InNo=" + InNo;
                    $('.loading-wrap').hide();
                }

            })
        });

        //新增資料
        $(function () {
            $('.tool-btn-add').on('click', function () {
                $(".popup-wrap.check").addClass("is-active");
                $(".popup-overlay").addClass("is-active");

                //取消
                $('.btn-cancel.small.check').on('click', function () {

                    $(".popup-wrap.check").removeClass("is-active");
                    $(".popup-overlay").removeClass("is-active");
                });

                //確定
                $('.btn-submit.small.check').on('click', function () {

                    $(".popup-wrap.check").removeClass("is-active");
                    $(".popup-overlay").removeClass("is-active");

                    window.location.href = "@Url.Action("Add", "In")";
                });
            });
        });

        //刪除資料
        $('.tool-btn-del').on('click', function () {
            $(".popup-wrap.check").addClass("is-active");
            $(".popup-overlay").addClass("is-active");

            //取消
            $('.btn-cancel.small.check').on('click', function () {
                $(".popup-wrap.check").removeClass("is-active");
                $(".popup-overlay").removeClass("is-active");
            });

            //確定
            $('.btn-submit.small.check').on('click', function () {
                $(".popup-wrap.check").removeClass("is-active");
                $(".popup-overlay").removeClass("is-active");

                let inNo = $("#InNo").val()

                if (inNo != '') {
                    $.ajax({
                        url: "@Url.Action("Delete", "In")",
                        type: 'POST',
                        data: { InNo: inNo },
                        success: function (data) {
                            if (data) {
                                alert("刪除成功!");

                                window.location.href = "@Url.Action("Index", "In")";
                            }
                            else {
                                alert("刪除失敗!");
                            }
                        }
                    });
                }
                else {
                    alert("找不到刪除資料!");
                }
            });
        });

        //修改資料
        $('.tool-btn-edit').on('click', function () {
            $(".popup-wrap.check").addClass("is-active");
            $(".popup-overlay").addClass("is-active");

            //取消
            $('.btn-cancel.small.check').on('click', function () {

                $(".popup-wrap.check").removeClass("is-active");
                $(".popup-overlay").removeClass("is-active");
            });

            //確定
                $('.btn-submit.small.check').on('click', function () {

                    $(".popup-wrap.check").removeClass("is-active");
                    $(".popup-overlay").removeClass("is-active");

                    let inNo = $("#InNo").val();

                    if (!inNo) {
                        alert("找不到編輯資料!");
                    }
                    else {
                        window.location.href = "@Url.Action("Edit", "In")/?InNo=" + inNo;
                    }
                });
        });

        //首筆資料
        $(function () {
            $('.tool-btn-first').on('click', function () {
                //先清空錨點
                let firstDataItem = $('.data-item.active');

                if (firstDataItem.length != 0) {
                    firstDataItem.removeClass('active');
                }

                $('.data-item').each(function (i, v) {
                    if (i === 0) {
                        let firstItem = $(v).children('.f-1.in10').text();

                        $(v).addClass('active');

                        let el = document.querySelector('.data-item.active');

                        el.scrollIntoView(true);

                        window.location.href = "@Url.Action("Index", "In")/?InNo=" + firstItem;
                    }
                });
            });
        });

        //末筆資料
        $(function () {
            $('.tool-btn-last').on('click', function () {
                //先清空錨點
                let lastDataItem = $('.data-item.active');

                if (lastDataItem.length != 0 ) {
                    lastDataItem.removeClass('active');
                }

                //找出資料總比數
                let dataItem = $('.data-item');
                let length = dataItem.length;

                //找出最後一筆資料
                dataItem.each(function (i, v) {
                    if (i === (length - 1)) {
                        let inNo = $(v).children('.f-1.in10').text();
                        $(v).addClass('active');

                        let el = document.querySelector('.data-item.active');

                        el.scrollIntoView(true);

                        window.location.href = "@Url.Action("Index", "In")/?InNo=" + inNo;
                    }
                });
            });
        });

        //下一筆資料
        $(function () {
            $(".tool-btn-next").on('click', function () {

                if ($('.data-item.active').length == 0) {
                    alert("請選擇起始資料");

                    return;
