                                    {
                                        foreach (var item in Model.in10List)
                                        {
                                            <div class="data-item">
                                                <div class="f-1 in10 ta-left">@item.InNo</div>
                                            </div>
                                        }
                                    }
                                </div>
                            </div>
                        </div>
                        <div class="container-col-2">
